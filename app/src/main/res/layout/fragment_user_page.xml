<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserPageFragment"


    >

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_planai"
        android:layout_width="150dp"
        android:layout_height="50dp"

        android:layout_marginStart="54dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="54dp"
        android:background="@drawable/prideti_button"
        android:backgroundTint="@color/white"
        android:text="@string/planai"
        android:textStyle="bold"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_treneriai"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="36dp"
        android:textStyle="bold"
        android:background="@drawable/prideti_button"
        android:backgroundTint="@color/white"
        android:elevation="3dp"
        android:text="@string/treneriai"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="438dp"
        android:layout_height="743dp"
        android:src="@drawable/Planai"
        android:adjustViewBounds="true"
        android:scaleType="centerInside"
        tools:layout_editor_absoluteX="-14dp"
        tools:layout_editor_absoluteY="-163dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="463dp"
        android:layout_height="638dp"
        android:adjustViewBounds="true"
        android:scaleType="centerInside"
        android:src="@drawable/Treneriai"
        tools:layout_editor_absoluteX="-26dp"
        tools:layout_editor_absoluteY="380dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SmartFit"
        android:textColor="@color/white"
        android:textSize="50sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="380dp" />
</androidx.constraintlayout.widget.ConstraintLayout>
